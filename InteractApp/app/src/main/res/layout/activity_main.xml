<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.example.wenwei.interactapp.MainActivity">

    <Button
        android:id="@+id/call_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="call"
        android:onClick="callBtnClick"/>

    <Button
        android:id="@+id/open_browser_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="browser"
        android:onClick="browserBtnClick"/>

    <Button
        android:id="@+id/email_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Email"
        android:onClick="emailBtnClick"/>

    <Button
        android:id="@+id/calendar_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Calendar"
        android:onClick="calendarBtnClick"/>

    <Button
        android:id="@+id/pick_calendar_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Pick Contacts"
        android:onClick="pickContactClick"/>

</LinearLayout>
